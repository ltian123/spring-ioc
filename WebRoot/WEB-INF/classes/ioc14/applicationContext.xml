<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
<!--  FactoryBean  -->
	
	<bean id="parser" class="ioc14.SAXParserFactoryBean"></bean>
	
	<bean id="date" class="ioc14.DateFactoryBean">
		<property name="year" value="2016"></property>
		<property name="month" value="11"></property>
		<property name="day" value="16"></property>
	</bean>

</beans>